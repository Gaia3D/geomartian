{
	"tags": {
		"allowUnknownTags": true
	},
	"opts": {
		"destination": "./build/documention"
	},
	"plugins": [
		"plugins/markdown"
		],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"systemName" : "Mago3dJS for SeaForest",
		"default": {
			"outputSourceFiles": true
		},
		"path": "ink-docstrap",
		"theme": "readable",
		"navType": "vertical",
		"linenums": true,
		"dateFormat": "YYYY, MMMM Do h:mm:ss a",
		"outputSourceFiles":true,
		"outputSourcePath":true
	}
}