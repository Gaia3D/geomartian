{
	"tags": {
		"allowUnknownTags": true
	},
	"opts": {
		"destination": "./doc/",
		"template": "./node_modules/clean-jsdoc-theme",
		"theme_opts":{
			"theme": "light"
		}
	},
	"plugins": [
		"plugins/markdown"
		],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"systemName" : "Mago3dJS",
		"default": {
			"outputSourceFiles": true
		},
		"navType": "vertical",
		"linenums": true,
		"dateFormat": "YYYY, MMMM Do h:mm:ss a",
		"outputSourceFiles":true,
		"outputSourcePath":true
	}
}